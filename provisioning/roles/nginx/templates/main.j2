server {
    listen 443 default_server;
    listen [::]:443 default_server ipv6only=on;

    server_name _ ;

    include extra/ssl;
    include extra/security;

    location / {
        proxy_pass https://mayurifag.github.io;
        proxy_set_header Host mayurifag.github.io;
    }
}

include extra/redirect_to_https;
